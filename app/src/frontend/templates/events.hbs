{{#> layout}}
  <h1>Scheduled Events</h1>
  <ul>
    {{#each events}}
      <li>
        <h2>{{this.name}}</h2>
        <p>{{this.description}}</p>
        <p><strong>Location:</strong> {{this.location}}</p>
        <p><strong>Date:</strong> {{this.date}}</p>
        <p><strong>Required Skills:</strong> {{this.skills}}</p>
      </li>
    {{/each}}
  </ul>
{{/layout}}
